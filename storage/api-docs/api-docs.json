{
    "openapi": "3.0.0",
    "info": {
        "title": "Kapusta api",
        "description": "Documentation Kapusta API",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://31.128.46.70",
            "description": "Debug test server"
        }
    ],
    "paths": {
        "/api/v1/register": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Получить детальные данные для страницы essence",
                "operationId": "6796c44e1f6079c411abbfed066c5907",
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "description": "Email",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "test@mail.ru"
                    },
                    {
                        "name": "name",
                        "in": "query",
                        "description": "Имя",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "Александр Александров"
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "description": "Пароль",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "123qweasd"
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Пользователь зарегистрирован, а код отправлен ему на почту",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "userId": {
                                            "type": "integer",
                                            "example": 1
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Ошибка регистрации",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Произошла ошибка"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "auth_api": []
                    }
                ]
            }
        },
        "/api/v1/check-code": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Проверить код регистрации",
                "operationId": "05ff8b650c30642d6b4e13cfb2da9cc8",
                "parameters": [
                    {
                        "name": "code",
                        "in": "query",
                        "description": "Код",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "55555"
                    },
                    {
                        "name": "userId",
                        "in": "query",
                        "description": "ID пользователя полученный после регистрации",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        },
                        "example": "1"
                    }
                ],
                "responses": {
                    "204": {
                        "description": "Код верный"
                    },
                    "400": {
                        "description": "Неверный код"
                    }
                },
                "security": [
                    {
                        "auth_api": []
                    }
                ]
            }
        },
        "/api/v1/login": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Авторизация",
                "operationId": "da2ca4d7a5cdec77b8b45a20373535cb",
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "description": "Email",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "test@mail.ru"
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "description": "Пароль",
                        "required": true,
                        "schema": {
                            "type": "string"
                        },
                        "example": "123qweasd"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Успешно",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "token": {
                                            "description": "Токен авторизации",
                                            "type": "string",
                                            "example": "AscaksSADkaskdSQWe"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Неверный логин или пароль",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "Неверный логин или пароль"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "auth_api": []
                    }
                ]
            }
        }
    },
    "components": {
        "schemas": {
            "ExpenceRequest": {
                "description": "Данные для затрат",
                "properties": {
                    "price": {
                        "type": "number",
                        "format": "float",
                        "example": "500.00"
                    },
                    "date": {
                        "type": "string",
                        "format": "string",
                        "example": "1000.00"
                    },
                    "comment": {
                        "type": "string",
                        "format": "string",
                        "example": "50"
                    },
                    "projectId": {
                        "type": "integer",
                        "format": "integer",
                        "example": "50",
                        "nullable": true
                    },
                    "categoryId": {
                        "type": "integer",
                        "format": "integer",
                        "example": "50"
                    },
                    "accountId": {
                        "type": "integer",
                        "format": "integer",
                        "example": "50"
                    }
                },
                "type": "object"
            },
            "ProjectRequest": {
                "description": "Данные для создания проекта",
                "properties": {
                    "budget": {
                        "type": "number",
                        "format": "float",
                        "example": "1000.00"
                    },
                    "name": {
                        "type": "string",
                        "format": "string",
                        "example": "Car"
                    },
                    "expenses": {
                        "type": "array",
                        "format": "array",
                        "items": {
                            "$ref": "#/components/schemas/ExpenceRequest"
                        }
                    }
                },
                "type": "object"
            },
            "Category": {
                "description": "Информация о категории",
                "properties": {
                    "name": {
                        "type": "string",
                        "format": "string",
                        "example": "food"
                    },
                    "purchaseCount": {
                        "type": "integer",
                        "format": "integer",
                        "example": "15"
                    },
                    "icon": {
                        "type": "string",
                        "format": "string",
                        "example": "https://example.com/path/to/file.jpg"
                    },
                    "totalExpenses": {
                        "type": "number",
                        "format": "float",
                        "example": "150.00"
                    },
                    "personOfBudget": {
                        "type": "integer",
                        "format": "integer",
                        "example": "20"
                    }
                },
                "type": "object"
            },
            "ProjectLimits": {
                "description": "Информация о лимитах",
                "properties": {
                    "spent": {
                        "type": "number",
                        "format": "float",
                        "example": "500.00"
                    },
                    "limit": {
                        "type": "number",
                        "format": "float",
                        "example": "1000.00"
                    },
                    "percent": {
                        "type": "integer",
                        "format": "integer",
                        "example": "50"
                    }
                },
                "type": "object"
            },
            "ProjectDetail": {
                "description": "Детальная информация проекта",
                "properties": {
                    "totalBalance": {
                        "type": "number",
                        "format": "float",
                        "example": "1000.00"
                    },
                    "name": {
                        "type": "string",
                        "format": "string",
                        "example": "Car"
                    },
                    "expenses": {
                        "type": "number",
                        "format": "float",
                        "example": "60000.00"
                    },
                    "limits": {
                        "$ref": "#/components/schemas/ProjectLimits"
                    },
                    "categories": {
                        "type": "array",
                        "format": "array",
                        "items": {
                            "$ref": "#/components/schemas/Category"
                        }
                    },
                    "incomes": {
                        "type": "array",
                        "format": "array",
                        "items": {
                            "type": "string"
                        },
                        "nullable": true
                    }
                },
                "type": "object"
            },
            "ProjectList": {
                "description": "Список проектов",
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int",
                        "example": "1"
                    },
                    "name": {
                        "type": "string",
                        "format": "string",
                        "example": "Car"
                    },
                    "spent": {
                        "type": "number",
                        "format": "float",
                        "example": "600.00"
                    },
                    "budget": {
                        "type": "number",
                        "format": "float",
                        "example": "60000.00"
                    },
                    "percent": {
                        "type": "integer",
                        "format": "int",
                        "example": "50"
                    }
                },
                "type": "object"
            },
            "User": {
                "description": "Информация о пользователе",
                "properties": {
                    "name": {
                        "type": "string",
                        "format": "string",
                        "example": "Александров Александр"
                    },
                    "email": {
                        "type": "string",
                        "format": "string",
                        "example": "test@mail.ru"
                    }
                },
                "type": "object"
            }
        }
    },
    "tags": [
        {
            "name": "Auth",
            "description": "Авторизация и регистрация"
        }
    ]
}